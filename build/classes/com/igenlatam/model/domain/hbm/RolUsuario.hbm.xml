<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.igenlatam.model.domain.RolUsuario" table="rol_usuario">
        <id name="rolUsuarioId" type="java.lang.Long">
            <column name="rol_usuario_id" />
            <generator class="increment" />
        </id>
        <property name="descripcionRol" type="java.lang.String">
            <column name="descripcion_rol" />
        </property>
        <property name="usuarioCreacion" type="java.lang.String">
            <column name="usuario_creacion" />
        </property>      
          <property name="usuarioModificacion" type="java.lang.String">
            <column name="usuario_modificacion" />
        </property>   
          <property name="fechaCreacion" type="java.util.Date">
            <column name="fecha_creacion" />
        </property>      
          <property name="fechaModificacion" type="java.util.Date">
            <column name="fecha_modificacion" />
        </property> 
    </class>
</hibernate-mapping>
